@model IEnumerable<StudentModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome!!</h1>
    <table>
        <thead>
            <tr>
                <th>StudentId</th>
                <th>FName</th>
                <th>Last Name</th>
                <th>Gender</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.StudentId</td>
                <td>@item.FName</td>
                <td>@item.LName</td>
                <td>@item.Gender</td>

            </tr>

            }
        </tbody>
    </table>
 
</div>
